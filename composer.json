{
	"name": "evoweb/sf-oauth",
	"type": "typo3-cms-extension",
	"description": "Oauth for frontend",
	"homepage": "https://github.com/evoWeb/sf_oauth/",
	"license": ["GPL-2.0+"],

	"support" : {
		"issues" : "https://github.com/evoWeb/sf_oauth/issues"
	},
	"keywords": [
		"oauth"
	],

	"require": {
		"typo3/cms-core": ">=8.0.0",
		"league/oauth2-client": "^2.2.1"
	},

	"autoload": {
		"psr-4": {
			"Evoweb\\SfOauth\\": "Classes/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Evoweb\\SfOauth\\Tests\\": "Tests/"
		}
	},

	"config": {
		"vendor-dir": ".Build/vendor",
		"bin-dir": ".Build/bin"
	},
	"extra": {
		"typo3/cms": {
			"cms-package-dir": "{$vendor-dir}/typo3/cms",
			"web-dir": ".Build/Web"
		}
	}
}
